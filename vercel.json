{
  "buildCommand": "turbo build",
  "outputDirectory": "apps/web/dist",
  "framework": null,
  "installCommand": "bun install",
  "bunVersion": "1.x",
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/client/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/server"
    }
  ],
  "functions": {
    "server/server.js": {
      "runtime": "bun@1",
      "includeFiles": "**"
    }
  }
}